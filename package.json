    "jest-environment-enzyme": "^7.1.0",
    "jest-enzyme": "^7.1.0",
    "lint-staged": "^9.2.3",
    "mockjs": "^1.0.1-beta3",
    "prettier": "^1.18.2",
    "react-app-rewired": "^2.1.3",
    "standard-version": "^7.0.0",
    "webpack-bundle-analyzer": "^3.4.1",
    "worker-loader": "^2.0.0"
